[group:fifoci]
programs=gunicorn_fifoci

[program:gunicorn_fifoci]
command=gunicorn -c gunicorn.conf.py -p gunicorn.pid fifoci.wsgi
directory=/home/fifoci/fifoci/frontend
user=fifoci
autostart=true
autorestart=true
redirect_stderr=true
environment=LANG="en_US.UTF-8",LC_ALL="en_US.UTF-8",LC_LANG="en_US.UTF-8"

